{
  "name": "nextjs-assistant",
  "displayName": "Next.js Assistant",
  "publisher": "nrjdalal",
  "icon": "dist/icon.png",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.4.9"
  },
  "scripts": {
    "generate": "rm -rf ./dist/ && bun generate.js && cp ./assets/* ./dist/ && bunx prettier --write ./dist/*.code-snippets",
    "publish": "bun run generate && vsce publish minor"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [],
  "contributes": {
    "snippets": [
      {
        "language": "typescriptreact",
        "path": "./dist/nrjdalal.code-snippets"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nrjdalal/nextjs-assistant"
  },
  "bugs": {
    "url": "https://github.com/nrjdalal/nextjs-assistant/issues"
  }
}
