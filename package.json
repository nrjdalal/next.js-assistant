{
  "name": "nextjs-assistant",
  "displayName": "Next.js Assistant (Beta)",
  "description": "The latest and most reliable Next.js snippets for VS Code",
  "publisher": "nrjdalal",
  "icon": "dist/icon.png",
  "version": "0.4.0",
  "engines": {
    "vscode": "^1.4.9"
  },
  "scripts": {
    "generate": "rm -rf ./dist/ && bun generate.js && cp ./assets/* ./dist/ && bunx prettier --write ./dist/*.code-snippets",
    "publish": "bun run generate && vsce publish minor"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [],
  "contributes": {
    "snippets": [
      {
        "language": "typescript",
        "path": "./dist/ts.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./dist/tsx.code-snippets"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nrjdalal/next.js-assistant"
  },
  "bugs": {
    "url": "https://github.com/nrjdalal/next.js-assistant/issues"
  }
}
